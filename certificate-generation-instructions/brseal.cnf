oid_section = new_oids

[ new_oids ]
atributos-obrigatorios_nome_cnpj  = 2.16.76.1.3.2
atributos-obrigatorios_cnpj  = 2.16.76.1.3.3
atributos-obrigatorios_dados_pj  = 2.16.76.1.3.4
atributos-obrigatorios_cei_pj  = 2.16.76.1.3.7

[ req ]
default_bits             = 2048                  # RSA key size
encrypt_key              = no                    # Protect private key: yes or no. yes recommended
default_md               = sha256                # MD to use. sha256 recommended
utf8                     = yes                   # Input is UTF-8.
string_mask              = utf8only              # Emit UTF-8 strings
prompt                   = no                    # Prompt for DN. yes or no. 
distinguished_name       = client_dn             # DN template. Mandatory to include organizationIdentifier
req_extensions           = client_reqext         # Desired extensions


[ client_dn ]

#Fixed Value
countryName              = BR

#Fixed Value
organizationName         = ICP-Brazil

#Organisation Name from Directory
commonName               = Raidiam Test Org

#Organisation ID
UID                      = 94271194-ad90-4c39-b564-a080e7cb0bf1

#Authority name of an authority associated with the organisation
0.organizationalUnitName = Directory OpenBanking BR AC

#Authority CNPJ (not validated right now, must be present)
1.organizationalUnitName = 64721803000118

#Validation Type (not validated right now, must be present)
2.organizationalUnitName = certificado digital

[ client_reqext ]
keyUsage                 = critical, digitalSignature, nonRepudiation
subjectKeyIdentifier     = hash
basicConstraints         = CA:FALSE
subjectAltName           = @alt_names

[alt_names]
# Name of individual responsible for certificate - (not validated right now, must be present)
otherName.1=atributos-obrigatorios_nome_cnpj;UTF8:Bob Smith

# Organisation Registration Number
otherName.2=atributos-obrigatorios_cnpj;UTF8:0800001066

# CPF of individual responbsible for certificate - (not validated right now, must be present)
otherName.3=atributos-obrigatorios_dados_pj;UTF8:963.572.323-78

# Number of the Specific Register of INSS (CEI) of the legal person holding the certificate.(not validated right now, must be present)
otherName.4=atributos-obrigatorios_cei_pj;UTF8:abc123

